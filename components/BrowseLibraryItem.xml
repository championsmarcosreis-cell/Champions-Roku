    <Poster
      id="icon"
      translation="[16, 18]"
      width="20"
      height="20"
      uri="pkg:/images/tab_movies_white.png"
    />

    <Label
      id="label"
      translation="[44, 16]"
      width="140"
      height="24"
      font="font:MediumSystemFont"
      color="0xFFFFFF"
      horizAlign="left"
      text=""
    />
